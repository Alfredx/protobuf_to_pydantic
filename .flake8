[flake8]
max-line-length = 120
max-complexity = 24
ignore = F401, W503, E203
exclude =
    .git,
    .venv,
    __pycache__,
    scripts,
    logs,
    upload,
    build,
    dist,
    docs,
    migrations,
    *pb2.py,
    ^example/example_proto_python_code/example_proto,
    ^tests,
    ^protobuf_to_pydantic/protos
